<h4><%=@organizations.name %></h4>
<table class="table table-hover table-dark">
  <thread>
    <tr>
      <th scope="col">Users</th>
    </tr>
  </thread>
  <%- @user.each do |user|%>
    <tbody>
    <tr data-item_id="<%= user.name %>">
      <th scope="col"><%= link_to user.name,organization_user_path(id:user)%></th>
    </tr>
    </tbody>
  <% end %>
</table>

<div class ="btn-toolbar mb-3" role="toolbar" aria-label="Toolbar with button groups">
  <div class ="btn-toolbar mr-2" role="group" aria-label="First group">
    <%= button_to 'Create User', new_organization_user_path, method: :get,
                  class: "btn btn-secondary", style: "margin-right: 15px"%>
  </div>
</div>
